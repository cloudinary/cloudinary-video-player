{
  "name": "cloudinary-video-player",
  "version": "1.1.2-edge.3",
  "description": "Cloudinary Video Player",
  "scripts": {
    "postinstall": "cp env.example.js env.js",
    "start": "webpack-dev-server --config webpack/dev.config.js --watch",
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production webpack --config webpack/es5.config.js --progress --display-error-details --color",
    "build-light": "NODE_ENV=production WEBPACK_BUILD_LIGHT=1 webpack --config webpack/es5.config.js --progress --display-error-details --color",
    "build-minified": "NODE_ENV=production webpack --config webpack/minified.config.js --progress --display-error-details --color",
    "build-minified-light": "NODE_ENV=production WEBPACK_BUILD_LIGHT=1 webpack --config webpack/minified.config.js --progress --display-error-details --color",
    "analyze": "NODE_ENV=production webpack --config webpack/minified.config.js --json | webpack-bundle-size-analyzer",
    "build-all": "npm run clean && npm run build && npm run build-light && npm run build-minified && npm run build-minified-light",
    "precommit": "eslint src",
    "predeploy": "deploy=true npm run build-all",
    "deploy": "deploy=true node webpack/deploy.js",
    "postdeploy": "git push origin master --follow-tags",
    "dry:deploy": "node webpack/deploy.js dry",
    "test": "jest --no-cache --detectOpenHandles"
  },
  "author": "Cloudinary",
  "license": "MIT",
  "main": "./dist/cld-video-player.js",
  "style": "./dist/cld-video-player.css",
  "dependencies": {
    "expose-loader": "^0.7.5",
    "fetch-ponyfill": "^4.0.0",
    "get-size": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "promise-polyfill": "^6.0.2",
    "video.js": "^7.4.1",
    "videojs-contextmenu": "^1.2.2",
    "videojs-contrib-ads": "^6.6.1",
    "videojs-ima": "^1.5.1",
    "videojs-per-source-behaviors": "^2.1.0",
    "webfontloader": "^1.6.28",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-conditional-loader": "^1.0.11",
    "webworkify-webpack2": "^1.1.9",
    "wolfy87-eventemitter": "^5.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "deep-assign": "^2.0.0",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-jest": "^21.22.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.4.0",
    "jest-webpack": "^0.5.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^1.3.0",
    "puppeteer": "^1.8.0",
    "puppeteer-request-spy": "^1.1.2",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^3.4.7",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1",
    "webpack-shell-plugin": "^0.5.0"
  }
}
