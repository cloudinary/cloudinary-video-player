{
  "name": "cloudinary-video-player",
  "version": "1.2.0",
  "description": "Cloudinary Video Player",
  "scripts": {
    "prepare": "shx cp env.example.js env.js",
    "start": "webpack-dev-server --config webpack/dev.config.js",
    "clean": "rm -rf dist",
    "build-dev": "webpack --config webpack/es5.config.js --progress --display-error-details --color --mode=development",
    "build": "webpack --config webpack/es5.config.js --progress --display-error-details --color",
    "build-light": "WEBPACK_BUILD_LIGHT=1 webpack --config webpack/es5.config.js --progress --display-error-details --color",
    "analyze": "webpack --config webpack/minified.config.js --json | webpack-bundle-size-analyzer",
    "build-all": "npm run clean && npm run build-dev && npm run build && npm run build-light",
    "precommit": "eslint src",
    "predeploy": "deploy=true npm run build-all",
    "deploy": "deploy=true node webpack/deploy.js",
    "postdeploy": "git push origin master --follow-tags",
    "dry:deploy": "node webpack/deploy.js dry",
    "test": "jest --no-cache --detectOpenHandles"
  },
  "author": "Cloudinary",
  "license": "MIT",
  "main": "./dist/cld-video-player.min.js",
  "style": "./dist/cld-video-player.min.css",
  "dependencies": {
    "expose-loader": "^0.7.5",
    "fetch-ponyfill": "^4.0.0",
    "get-size": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "promise-polyfill": "^6.0.2",
    "video.js": "^7.4.1",
    "videojs-contextmenu": "^1.2.2",
    "videojs-contrib-ads": "^6.6.1",
    "videojs-ima": "^1.5.1",
    "videojs-per-source-behaviors": "^2.1.0",
    "webfontloader": "^1.6.28",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-conditional-loader": "^1.0.11",
    "webworkify-webpack2": "^1.1.9",
    "wolfy87-eventemitter": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "autoprefixer": "^6.7.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.22.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.26.1",
    "deep-assign": "^2.0.0",
    "eslint": "^5.12.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^21.22.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "jest-puppeteer": "^3.4.0",
    "jest-webpack": "^0.5.1",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.7.2",
    "puppeteer": "^1.8.0",
    "puppeteer-request-spy": "^1.1.2",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.0",
    "shx": "^0.3.2",
    "terser-webpack-plugin": "^1.2.1",
    "uglify-js": "^3.4.7",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-shell-plugin": "^0.5.0"
  }
}
